{
  "name": "delver",
  "repo": "https://github.com/DavidMallett/delver.git",
  "version": "0.0.1",
  "description": "delver: the sequel to learning-angular; in which we learn how to automate various build pipelines, including sourcemaps, obfuscation, and more!",
  "main": "index.js",
  "ava": {},
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended"
    ],
    "parser": "typescript-eslint-parser",
    "plugins": [
      "prettier",
      "typescript"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "overrides": [
      {
        "files": "package.json",
        "options": {
          "parser": "json",
          "printWidth": 72
        }
      }
    ],
    "printWidth": 80,
    "proseWrap": "always"
  },
  "scripts": {
    "lint": "node ./node_modules/eslint/bin/eslint.js \"src/**/*.ts\"",
    "prettify": "node ./node_modules/prettier/bin-prettier.js --config package.json \"src/**/*.ts\"",
    "test": "unittest && ava"
  },
  "author": "David Mallett",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.10",
    "mtgsdk": "^0.3.0",
    "mtgsdk-ts": "^1.1.2",
    "node-json-db": "^0.7.5",
    "request": "^2.87.0",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.56",
    "@babel/core": "^7.0.0-beta.56",
    "@babel/parser": "^7.0.0-beta.56",
    "@babel/plugin-transform-typescript": "^7.0.0-beta.56",
    "@babel/polyfill": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "@babel/preset-typescript": "^7.0.0-beta.56",
    "@babel/types": "^7.0.0-beta.56",
    "@types/chai": "^4.1.4",
    "@types/es6-promise": "^3.3.0",
    "@types/eslint": "^4.16.3",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/gulp": "^4.0.5",
    "@types/gulp-mocha": "0.0.32",
    "@types/lodash": "^4.14.113",
    "@types/mocha": "^5.2.5",
    "@types/moment": "2.13.0",
    "@types/node": "^10.5.3",
    "ava": "1.0.0-beta.6",
    "chai": "^4.1.2",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-typescript": "^0.12.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-mocha": "^6.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-terser": "^1.0.1",
    "gulp-typescript": "^5.0.0-alpha.3",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.17",
    "mochawesome": "^3.0.3",
    "moment": "^2.22.2",
    "nyc": "^12.0.2",
    "prettier": "1.14.0",
    "pump": "^3.0.0",
    "terser": "^3.8.1",
    "ts-node": "^7.0.0",
    "typescript-eslint-parser": "^18.0.0"
  }
}
